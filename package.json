    "eslint": "^9.37.0",
    "eslint-plugin-vue": "^9.17.0",
    "prettier": "^3.6.2",
    "sass": "^1.83.0",
    "sass-embedded": "^1.83.0",
    "sass-loader": "^16.0.4",
    "vite": "^7.1.9",
    "vite-plugin-vue-devtools": "^8.0.2"
